# Vulcan Tank 2 (hot atmo)
alias Tank d0
alias Pump d1
# Alias Register
alias Tmin r0
alias Tcur r1
alias Pcur r2
alias IsNotFull r3
alias IsTemp r4
alias Temp r15
# Constant
define TYPE_SENSOR -1252983604
define KELVIN 273
define TMIN 550
define PMAX 10000
# Settings
add Tmin TMIN KELVIN
s Pump Setting 100
start:
yield
lb Tcur TYPE_SENSOR Temperature 1
l Pcur Tank Pressure
slt IsNotFull Pcur PMAX
sgt IsTemp Tcur Tmin
and Temp IsTemp IsNotFull
s Pump On Temp
j start