# silo_packet_provider.ic10
alias Silo0 d0
alias Silo1 d1
alias Silo2 d2
alias Silo3 d3
alias Silo4 d4
alias Silo5 d5
#Constant
define STACK_ORE 50
define STACK_INGOT 100
define QUERY_LIST 9001100
define FREE 9000000
define SLEEP 0.05
define NONE -3
define START_SP 1 # 1,7:Ores 13,19,25:Ingots
define PACKET_SP 250
define PACKET_SIZE 12
# Alias Register
alias Device r0
alias Request r2
alias Index r3
alias Qty r4
alias Hash r5
alias Stack r6
alias SpEnd r7
alias Temp r15
# Settings
sle Temp START_SP 7
select Stack Temp STACK_ORE STACK_INGOT
jal push_hash_ingot
s db Setting FREE # Write line
start:
yield
l Request db Setting
beq Request 0 start # Wait Request
beq Request FREE start # Wait Request
bne Request QUERY_LIST init # No Response
jal read_devices
s db Setting FREE # Write line
j start
init:
s db Setting 0
j start

read_devices:
move Device -1 # Reset Index
move Qty 0 # Reset Quantity
loop_device:
add Device Device 1
bgt Device 5 ra # exit
bdns dr0 loop_device # if no device exit
l Qty dr0 Quantity
mul Qty Qty Stack
add sp Device START_SP
peek Hash
s db Setting Hash # Write line
sleep SLEEP
s db Setting Qty # Write line
sleep SLEEP
j loop_device

push_hash_ingot:
move sp 0
# Ores
push 1724793494  # 1:Coal Ore
