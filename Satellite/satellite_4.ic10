# satellite_4.ic10
alias Dish0 d0
alias Dish1 d1
alias Dish2 d2
alias Dish3 d3
alias DialH d4
alias DialV d5
# Constant
define TYPE_DISH 439026183
# Alias Register
alias Device r0
alias H r1
alias V r2
alias Delta r3
# Settings
start:
yield
l H DialH Setting
l V DialV Setting
sb TYPE_DISH Vertical V
move Device -1
loop_device:
add Device Device 1
bgt Device 3 start # exit
mul Delta Device 90
add H H Delta
s dr0 Horizontal H
j loop_device