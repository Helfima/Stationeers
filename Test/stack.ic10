# Alias Register
alias Signal r4
alias Temp r10
alias IsSkip r13
alias Skip r14 #Stack index

move Skip -1
move IsSkip 0

start:
s db Setting -99999
yield
s db Setting Skip
yield
# Add 3 values in stack
move Signal 100
jal addSkip
move Signal 101
jal addSkip
move Signal 102
jal addSkip
# Set test value
move Signal 100
# Execute the test
jal checkSkip
s db Setting IsSkip
sleep 2
j start

addSkip:
s db Setting -11
yield
add Skip Skip 1
move sp Skip
push Signal
j ra

checkSkip:
s db Setting -10
yield
beq Skip -1 ra
move IsSkip 0
move sp Skip
ble sp 0 ra # End stack
pop Temp # Read stack value
brne Temp Signal -2
move IsSkip 1
j ra